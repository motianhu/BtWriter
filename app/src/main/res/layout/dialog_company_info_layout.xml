<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_corner_10"
    android:orientation="vertical"
    android:paddingBottom="@dimen/dimen_20dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15dp">

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dimen_15dp"
            android:text="@string/contact_mch"
            android:textColor="@color/color_222A37"
            android:textSize="@dimen/font_12" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/dimen_60dp"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:text="@string/distributor"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />

        <TextView
            android:id="@+id/distributor"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/dimen_60dp"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:text="@string/email"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />

        <TextView
            android:id="@+id/email"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/dimen_60dp"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:text="@string/phone"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />

        <TextView
            android:id="@+id/phone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/dimen_60dp"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:text="@string/company"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />

        <TextView
            android:id="@+id/company"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/dimen_60dp"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:text="@string/contact"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />

        <TextView
            android:id="@+id/contact"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/dimen_60dp"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:text="@string/account"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />

        <TextView
            android:id="@+id/account"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_48dp"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dimen_10dp"
            android:paddingEnd="@dimen/dimen_10dp"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/font_13" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_ok"
        android:layout_width="@dimen/dimen_100dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dimen_20dp"
        android:background="@drawable/bg_corner_maincolor"
        android:gravity="center"
        android:padding="@dimen/dimen_5dp"
        android:text="@string/action_ok"
        android:textColor="@color/white"
        android:textSize="@dimen/font_16" />

</LinearLayout>